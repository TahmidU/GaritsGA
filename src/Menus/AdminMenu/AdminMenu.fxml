<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Menus.AdminMenu.AdminMenuController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="600.0" prefWidth="800.0">
         <children>
            <TabPane fx:id="adminTab" layoutX="-5.0" layoutY="20.0" prefHeight="570.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
               <tabs>
                  <Tab text="Home">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                     </content>
                  </Tab>
                  <Tab text="Manage Accounts">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TableView fx:id="accountTable" layoutX="12.0" layoutY="14.0" prefHeight="306.0" prefWidth="782.0">
                                 <columns>
                                    <TableColumn fx:id="idCol" prefWidth="56.0" text="ID" />
                                    <TableColumn fx:id="usernameCol" prefWidth="101.0" text="Username" />
                                    <TableColumn fx:id="roleCol" prefWidth="102.0" text="Role" />
                                    <TableColumn fx:id="firstNameCol" prefWidth="98.0" text="First Name" />
                                    <TableColumn fx:id="lastNameCol" prefWidth="106.0" text="Last Name" />
                                    <TableColumn fx:id="phoneCol" prefWidth="132.0" text="Phone" />
                                    <TableColumn fx:id="emailCol" prefWidth="186.0" text="Email" />
                                 </columns>
                              </TableView>
                              <Button fx:id="addAccountButton" layoutX="65.0" layoutY="450.0" mnemonicParsing="false" onAction="#addAccountPress" prefHeight="35.0" prefWidth="114.0" text="Add Account" />
                              <Button layoutX="202.0" layoutY="450.0" mnemonicParsing="false" onAction="#editAccountPress" prefHeight="35.0" prefWidth="114.0" text="Edit Account" />
                              <Button layoutX="335.0" layoutY="450.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="114.0" text="Remove Account" />
                              <Label fx:id="testLabel" layoutX="142.0" layoutY="362.0" prefHeight="17.0" prefWidth="121.0" />
                           </children></AnchorPane>
                     </content>
                  </Tab>
                  <Tab text="Manage Database">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
               </tabs>
            </TabPane>
            <Label layoutX="5.0" text="Logged In as:" />
            <Label fx:id="loggedInAsText" layoutX="80.0" prefHeight="17.0" prefWidth="84.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
